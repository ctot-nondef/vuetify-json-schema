{
  "name": "Vuetify-json-schema",
  "version": "0.0.0-beta",
  "description": "Vue component form based on JSON Schema",
  "main": "dist/vuetify-json-schema.js",
  "scripts": {
    "pretest": "eslint --ext .js src test/specs",
    "test": "jest --colors",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "posttest": "make readme",
    "prepublish": "npm test",
    "build": "npm test && node build/build.js",
    "dev": "npm-run-all --parallel dev:lib dev:server",
    "dev:lib": "webpack --config build/webpack.dev.conf.js --watch --progress --hide-modules",
    "dev:server": "node build/dev-server.js",
    "tag": "git tag v`node -p 'require(\"./package\").version'`"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctot-nondef/vuetify-json-schema.git"
  },
  "keywords": [
    "vue",
    "vuetify",
    "form",
    "json",
    "schema"
  ],
  "author": "Christoph Hoffmann",
  "license": "MIT",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.11",
    "@vuedoc/md": "^1.1.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.7.0",
    "eventsource-polyfill": "^0.9.6",
    "jasmine-core": "^2.8.0",
    "jest": "^22.0.6",
    "ora": "^1.3.0",
    "phantomjs-prebuilt": "^2.1.16",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "vue": "^2.5.13",
    "vuetify": "^1.0.0",
    "vue-template-compiler": "^2.5.13",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "ora": "^2.1.0",
    "postcss-safe-parser": "^4.0.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.0.3",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-svgicon": "^3.0.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vuetify": "^1.0.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },  
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!.eslintrc.js",
      "!build/build.js",
      "!build/webpack.config.js",
      "!dist/**",
      "!coverage/**",
      "!**/node_modules/**"
    ],
    "mapCoverage": true
  }
}
